    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.30",
    "morgan": "^1.10.1",
    "multer": "^1.4.4",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.2",
    "prisma": "^6.14.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "weaviate-ts-client": "^2.2.0"
  },
  "devDependencies": {
